        <platform>all</platform>
        <website/>
        <video_guide>https://youtu.be/0kFhoWahtFw</video_guide>
    </extension>
</addon>

    <addon id="repository.brizio-addonrepo" name="Brizio Addon Repo" version="0.1.0" provider-name="Brizio">
        <requires>
            <import addon="xbmc.addon" version="12.0.0"/>
        </requires>
        <extension point="xbmc.addon.repository" name="Brizio Addon Repo">
            <info compressed="false">https://brizioaddon.000webhostapp.com/repo/_repo/addons.xml</info>
            <checksum>https://brizioaddon.000webhostapp.com/repo/_repo/addons.xml.md5</checksum>
            <datadir zip="true">https://brizioaddon.000webhostapp.com/repo/_repo/</datadir>
            <hashes>false</hashes>
        </extension>
        <extension point="xbmc.addon.metadata">
            <summary>Film e Serie TV gratuiti!</summary>
            <description>Da questa repo puoi scaricare il famoso Brizio Addon!!</description>
            <platform>all</platform>
    </extension>
</addon>

